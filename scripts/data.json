  },
  {
    "datatype": "wiki_paragraph",
    "identifier": "wpar4",
    "values": {
      "_id": "h2g2_wiki_paragraph_4",
      "section_id": "{{wsec3}}",
      "text": "Ford comes from a small planet somewhere in the vicinity of the star Betelgeuse, and not Guildford like he usually claims. During his fifteen years stranded on Earth he wrote a comprehensive entry for the renowned Hitchhiker's Guide to the Galaxy.",
      "preceded_by_id": null,
      "succeeded_by_id": null
